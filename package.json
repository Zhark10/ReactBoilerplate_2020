{
  "name": "my-react-boilerplate-2020",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "new:shared": "cd ./scripts && ./new-shared-generate.sh",
    "new:page": "cd ./scripts && ./new-page-generate.sh",
    "env:dev": "cat dev.env > .env",
    "env:prod": "cat prod.env > .env",
    "start:dev": "yarn env:dev && yarn start",
    "start:prod": "yarn env:prod && yarn start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "connected-react-router": "6.6.1",
    "dotenv": "8.2.0",
    "history": "4.10.1",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "lodash": "4.17.15",
    "node-sass": "4.13.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "3.4.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-saga": "1.1.3",
    "ts-loader": "6.2.1",
    "typesafe-actions": "5.1.0",
    "typescript": "3.6.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.149",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-router-redux": "5.0.18",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5"
  }
}
