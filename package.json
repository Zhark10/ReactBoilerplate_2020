{
  "name": "my-react-boilerplate-2020",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Zharavin Arkady",
    "url": "https://github.com/Zhark10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:prod": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "new:shared": "cd ./scripts && ./new-shared-generate.sh",
    "new:page": "cd ./scripts && ./new-page-generate.sh",
    "new:modal": "cd ./scripts && ./new-modal-generate.sh",
    "new:entity": "cd ./scripts && ./new-redux-entity.sh",
    "env:staging": "cat staging.env > .env",
    "env:production": "cat production.env > .env",
    "gen:last-versions": "npm shrinkwrap && node ./scripts/lockdown-generation"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/polyfill": "7.10.1",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "axios": "0.19.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "firebase": "7.24.0",
    "google-maps-react": "2.0.6",
    "history": "4.10.1",
    "i18next": "19.5.0",
    "i18next-browser-languagedetector": "4.3.1",
    "i18next-http-backend": "1.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "6.10.0",
    "react-i18next": "11.7.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.3.0",
    "react-router-redux": "4.0.8",
    "react-scripts": "3.4.1",
    "react-virtualized": "9.21.2",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-axios-middleware": "4.0.1",
    "redux-saga": "1.1.3",
    "typesafe-actions": "5.1.0",
    "typescript": "3.7.5",
    "use-form": "1.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/history": "4.7.11",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.3.0",
    "@types/react-router-redux": "5.0.18",
    "@types/react-virtualized": "9.21.10",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5",
    "@types/reselect": "2.2.0",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "prettier": "2.0.5",
    "sass": "^1.49.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
