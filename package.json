{
  "name": "my-react-boilerplate-2020",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Zharavin Arkady",
    "url": "https://github.com/Zhark10"
  },
  "scripts": {
    "build:dev": "yarn env:dev && webpack --mode development",
    "build:release": "yarn env:prod && webpack --mode production",
    "start": "yarn env:dev && webpack-dev-server --mode development --open",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "new:shared": "cd ./scripts && ./new-shared-generate.sh",
    "new:page": "cd ./scripts && ./new-page-generate.sh",
    "new:modal": "cd ./scripts && ./new-modal-generate.sh",
    "new:entity": "cd ./scripts && ./new-redux-entity.sh",
    "env:dev": "cat dev.env > .env",
    "env:prod": "cat prod.env > .env"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.19.2",
    "connected-react-router": "6.6.1",
    "history": "4.10.1",
    "i18next": "^19.5.0",
    "i18next-browser-languagedetector": "^4.3.1",
    "i18next-http-backend": "^1.0.15",
    "lodash": "4.17.15",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-i18next": "^11.7.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "3.4.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "typesafe-actions": "5.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.149",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-router-redux": "5.0.18",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5",
    "@types/reselect": "2.2.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "8.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "3.0.6",
    "ts-loader": "^7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
